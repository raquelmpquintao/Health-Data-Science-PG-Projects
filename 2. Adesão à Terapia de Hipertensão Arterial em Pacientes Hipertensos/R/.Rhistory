# Supondo que as colunas Date.of.Admission e Discharge.Date sejam do tipo character (string)
# Convertendo para o tipo de dados Date
amostra$Date.of.Admission <- as.Date(amostra$Date.of.Admission, format = "%Y-%m-%d")
amostra$Discharge.Date <- as.Date(amostra$Discharge.Date, format = "%Y-%m-%d")
# Calculando a diferença entre as datas
amostra$Duracao.Internamento <- amostra$Discharge.Date - amostra$Date.of.Admission
amostra$Duracao.Internamento <- as.numeric(amostra$Duracao.Internamento)
# Exibindo o DataFrame com a nova coluna de duração do internamento
View(amostra)
#exploração inicial
# Gráfico de Boxplot
boxplot(Duracao.Internamento ~ Admission.Type, data = amostra,
main = "Boxplot da Duração de Internamento por Tipo de Admissão",
xlab = "Tipo de Admissão", ylab = "Duração de Internamento (dias)")
library(car)
leveneTest(Duracao.Internamento ~ Admission.Type, data = amostra)
# Teste de ANOVA
modelo_anova <- aov(Duracao.Internamento ~ Admission.Type, data = amostra)
summary(modelo_anova)
